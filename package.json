{
  "name": "ws-gestao",
  "version": "1.0.0",
  "description": "Aplicação WS-Gestão estilo Ionic",
  "type": "module",
  "scripts": {
    "generate-keys": "node scripts/generate-case.js",
    "clean-keys": "rm -f src/app/keys-local.js src/app/keys-reco.js",
    "dev": "npm run generate-keys && live-server src",
    "build": "npm run generate-keys && rm -rf public && mkdir public && cp -r src/* public",
    "start": "npm run generate-keys && npx live-server src --port=5500 --open=pages/login/login.html --watch=src"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "firebase": "^12.3.0"
  },
  "devDependencies": {
    "live-server": "^1.2.2"
  },
  "author": "Wellerson Jesus",
  "license": "MIT"
}
